/*
* Устанавливает биты в TWCR
*/
.macro TWSI_Byte_Transmit
	_call TWSI_Byte_Transmit_act
.endm
//____________________________________________________________________________________
//____________________________________________________________________________________
TWSI_Byte_Transmit_act:
	push r16
	ldi		r16, (1<<TWINT) | (1<<TWEN)		; формируем сигнал для передачи данных с TWDR
	WRITE_REG TWCR, r16						; отправляем по шине
	TWSI_TWINT_Flag
	pop r16
ret											; выход из подпрограммы
 